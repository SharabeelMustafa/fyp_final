    <title>Demo 2</title>
    <style>
        .bus-card {
            background-color: #f8f9fa;
            border-radius: 12px;
            box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
            padding: 20px;
            text-align: center;
            transition: transform 0.3s, box-shadow 0.3s;
            cursor: pointer;
        }

        .bus-card:hover {
            transform: scale(1.05);
            box-shadow: 0 8px 12px rgba(0, 0, 0, 0.2);
        }

        .bus-title {
            font-size: 1.5rem;
            color: #343a40;
            margin-bottom: 10px;
        }

        .bus-route {
            font-size: 1.2rem;
            color: #6c757d;
        }

        .bus-container {
            display: flex;
            flex-wrap: wrap;
            gap: 20px;
            justify-content: center;
            margin-top: 20px;
        }

        .bus-card img {
            width: 80px;
            height: 80px;
            margin-bottom: 10px;
        }
    </style>
</head>

